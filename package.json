{
  "name": "@youtogether/frontend",
  "license": "MIT",
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "typewind generate",
    "prepare": "husky install",
    "lint": "eslint \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "lint:fix": "eslint --fix \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "format": "prettier \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "format:fix": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "start:dev": "vite --host",
    "start:prod": "import-meta-env -x .env.production && vite preview",
    "start:storybook": "storybook dev -p 6006 --no-open",
    "build": "tsc && vite build",
    "build:storybook": "storybook build -o dist-storybook",
    "generate:typewind": "typewind generate",
    "generate:api": "orval",
    "generate:env-types": "import-meta-env-typescript -x .env.sample",
    "messages:extract": "lingui extract",
    "messages:compile": "lingui compile"
  },
  "typewind": {
    "showPixelEquivalents": true,
    "rootFontSize": 16
  },
  "dependencies": {
    "@effector/reflect": "^8.3.1",
    "@farfetched/core": "^0.8.12",
    "@farfetched/zod": "^0.8.12",
    "@import-meta-env/prepare": "^0.1.8",
    "@lingui/core": "^4.2.1",
    "@lingui/react": "^4.2.1",
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "atomic-router": "^0.8.0",
    "atomic-router-react": "^0.8.3",
    "clsx": "^1.2.1",
    "compose-function": "^3.0.3",
    "cva": "npm:class-variance-authority@^0.6.0",
    "effector": "^22.8.6",
    "effector-factorio": "^1.1.0",
    "effector-forms": "^1.3.2",
    "effector-react": "^22.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "history": "^5.3.0",
    "patronum": "^1.18.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typewind": "^0.1.3",
    "zod": "^3.21.4"
